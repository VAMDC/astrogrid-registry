<?xml version="1.0" encoding="UTF-8"?>
<!--+
    | Maven project description for the AstroGrid registry components.
    +-->
<project
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="maven-project.xsd">
	<extend>${basedir}/../../maven-base/project.xml</extend>
    <currentVersion>${astrogrid.registry.version}</currentVersion>

    <!--+
        | The external libraries required to build our components.
        +-->
    <dependencies>
        <!--+
            | The Axis webapp war file.
            +-->
        <dependency>
            <groupId>axis</groupId>
            <artifactId>axis</artifactId>
            <version>1.3</version>
            <type>war</type>
            <url>http://ws.apache.org/axis/index.html</url>
        </dependency>

    </dependencies>

 
</project>
