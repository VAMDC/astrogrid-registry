    <!-- 
         This describes the SIA capabilities: SimpleImageAccess is used instead
         of the generic Capability.  Note how the default namespace is changed
	 so that the special SIA metadata does not need namespace prefix
      -->
    <sia:SimpleImageAccess xmlns="http://www.ivoa.net/xml/SIA/v0.6" xmlns:sia="http://www.ivoa.net/xml/SIA/v0.6">
       <vr:StandardURL xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">http://www.us-vo.org/news/simspec.html</vr:StandardURL>
       <ImageServiceType>Pointed</ImageServiceType>
       <MaxQueryRegionSize>
          <long>360.0</long>
          <lat>180.0</lat>
       </MaxQueryRegionSize>
       <MaxImageExtent>
          <long>360.0</long>
          <lat>180.0</lat>
       </MaxImageExtent>
       <MaxImageSize>
          <long>5000</long>
          <lat>5000</lat>
       </MaxImageSize>
       <MaxFileSize>100000000</MaxFileSize>
       <MaxRecords>5000</MaxRecords>
    </sia:SimpleImageAccess>
    <ParamHTTP qtype="GET" xmlns="http://www.ivoa.net/xml/VODataService/v0.4">
      <vr:Invocation xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">Extended</vr:Invocation>
      <vr:AccessURL use="base" xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">http://adil.ncsa.uiuc.edu/vocone?</vr:AccessURL>
      <HTTPResults>text/xml</HTTPResults>
      <!-- 
           These are the supported input parameters taken from the ADIL
           SIA service's "Metadata" operation.  
         -->
      <Param>
        <vr:Name xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">POS</vr:Name>
        <vr:Description xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">Search Position in the form &quot;ra,dec&quot; where ra and dec are given in decimal degrees in the ICRS coordinate system.</vr:Description>
	<DataType arraysize="2">float</DataType>
        <Unit>degrees</Unit>
      </Param>
      <Param>
        <vr:Name xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">SIZE</vr:Name>
        <vr:Description xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">Size of search region in the RA and Dec. directions.</vr:Description>
	<DataType arraysize="2">float</DataType>
        <Unit>degrees</Unit>
      </Param>
      <Param>
        <vr:Name xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">FORMAT</vr:Name>
        <vr:Description xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">Requested format of images.</vr:Description>
	<DataType arraysize="*">char</DataType>
      </Param>
      <Param>
        <vr:Name xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">INTERSECT</vr:Name>
        <vr:Description xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">Choice of intersection of matched images with the region of interest.</vr:Description>
	<DataType arraysize="*">char</DataType>
      </Param>
      <Param>
        <vr:Name xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">FREQ</vr:Name>
        <vr:Description xmlns:vr="http://www.ivoa.net/xml/VOResource/v0.9">Frequency of observation.</vr:Description>
	<DataType>float</DataType>
      </Param>
    </ParamHTTP>
